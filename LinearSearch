// Online C++ compiler to run C++ program online
#include <iostream>
using namespace std;
int main() {
    int n;
    cin>>n;
    int a[n];
    for(int i=0;i<n;i++){
        cin>>a[i];
    }
    int num,c=0;
    cin>>num;
    for(int i=0;i<n;i++){
        if(num==a[i]){
            c+=1;
            cout<<i+1<<endl;
            break;
        }
    }
    if(c==0){
        cout<<"-1"<<endl;
    }
    return 0;
}
//time complexity  O(n)
