// Online C compiler to run C program online
#include <stdio.h>

int main() {
    int n;
    scanf("%d",&n);
    int arr[n],trr[n];
    for(int i=0;i<n;i++){
        scanf("%d",&arr[i]);
    }
    int k=0;
    for(int i=n-1;i>=0;i--){
        trr[k]=arr[i];
        k++;
    }
    for(int i=0;i<n;i++){
        printf("%d ",trr[i]);
    }
    return 0;
}
