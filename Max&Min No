#include <bits/stdc++.h>
using namespace std;

int main()
{
    int n;
    cin >> n;
    int a[n];
    for(int i=0;i<n;i++){
        cin>>a[i];
        
    }
    int maxNo=a[0];
    int minNo=a[0];
    for(int i=1;i<n;i++){
        maxNo=max(maxNo,a[i]);
        minNo=min(minNo,a[i]);
    }
    cout<<"max no is :"<<maxNo<<endl;
    cout<<"min no is :"<<minNo<<endl;
    return 0;
    
}
